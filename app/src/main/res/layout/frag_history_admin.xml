<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FBF8F5">

    <View
        android:id="@+id/view22"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:background="#1A1A1A"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btnAdminBack"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#00FFFFFF"
        android:padding="0.1dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/arrow" />

    <Button
        android:id="@+id/btnHistoryTabunganAdmin"
        android:layout_width="125dp"
        android:layout_height="40dp"
        android:backgroundTint="#002196F3"
        android:text="Tabungan"
        android:textColor="#2196F3"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btnHistoryBayarAdmin"
        app:layout_constraintTop_toTopOf="@+id/btnHistoryPinjamAdmin" />

    <Button
        android:id="@+id/btnHistoryBayarAdmin"
        android:layout_width="125dp"
        android:layout_height="40dp"
        android:backgroundTint="#002196F3"
        android:text="Pembayaran"
        android:textColor="#2196F3"
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/btnHistoryTabunganAdmin"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/btnHistoryPinjamAdmin"
        app:layout_constraintTop_toTopOf="@+id/btnHistoryPinjamAdmin" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginStart="16dp"
        android:gravity="center_vertical"
        android:text="History"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/btnAdminBack"
        app:layout_constraintStart_toEndOf="@+id/btnAdminBack"
        app:layout_constraintTop_toTopOf="@+id/btnAdminBack" />

    <Button
        android:id="@+id/btnHistoryPinjamAdmin"
        android:layout_width="125dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#0000CC08"
        android:text="Peminjaman"
        android:textColor="#2196F3"
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/btnHistoryBayarAdmin"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:strokeColor="#2196F3" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvPinjamAdmin"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view22" />

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view22">

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>